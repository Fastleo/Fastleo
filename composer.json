{
    "name": "fastleo/fastleo",
    "description": "Admin panel for laravel",
    "keywords": [
        "laravel",
        "fastleo",
        "admin",
        "cmf",
        "cms"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://fastleo.ru",
    "version": "2.4.0",
    "authors": [
        {
            "name": "Roman Zakharov",
            "email": "camanru@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "doctrine/dbal": "^2.11",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "maatwebsite/excel": "^3.1",
        "fresh/transliteration": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Fastleo\\Fastleo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fastleo\\Fastleo\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fastleo\\Fastleo\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=fastleo --force"
        ],
        "post-package-update": [
            "@php artisan vendor:publish --tag=fastleo"
        ]
    }
}
