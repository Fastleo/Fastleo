{
    "name": "fastleo/fastleo",
    "type": "library",
    "license": "CC-BY-SA-4.0",
    "version": "2.1.18",
    "authors": [
        {
            "name": "Roman Zakharov",
            "email": "camanru@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "doctrine/dbal": "^2.10",
        "intervention/image": "^2.5",
        "laravel/framework": "^6.5",
        "maatwebsite/excel": "^3.1",
        "fresh/transliteration": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Fastleo\\Fastleo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fastleo\\Fastleo\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fastleo\\Fastleo\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=fastleo --force"
        ],
        "post-package-update": [
            "@php artisan vendor:publish --tag=fastleo"
        ]
    }
}
